/* *********************************************************************************************** */

// #shader_type fragment

#version 330

/* *********************************************************************************************** */

uniform usampler2D texture0;

/* *********************************************************************************************** */

in VertexAttributes
{
  vec2 uv;
} vertex;

/* *********************************************************************************************** */

out vec4 fragment_colour;

/* *********************************************************************************************** */

void main()
{
  fragment_colour = texture(texture0, vertex.uv);
}

/* *********************************************************************************************** *
 *
 * End
 *
 * *********************************************************************************************** */
